    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'apx_shim'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=apx-shim"
                ],
                "filter": {
                    "name": "apx_shim",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'apx-ui'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=apx-ui",
                    "--package=apx-ui"
                ],
                "filter": {
                    "name": "apx-ui",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'apx-ui'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
